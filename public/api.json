{
    "openapi": "3.0.2",
    "info": {
	"version": "1.0",
	"title": "Sample text"
    },
    "paths": {
	"/entity": {
	    "post": {
		"x-mojo-to": "person#info",
		"operationId": "getInfo",
		"summary": "Gets metadata related to the person",
		"responses": {
		    "200": {
			"description": "Get name parts, rank, and who user has permissions to.",
			"content": {
			    "application/json": {
				"schema": {
				    "properties": {
					"name": {
					    "type": "array"
					},
					"rank": {
					    "type": "string"
					},
					"subordinates": {
					    "type": "object"
					},
					"documents": {
					    "type": "integer"
					}
				    }
				}
			    }
			}
		    },
		    "418": {
			"description": "Unsuccessful login",
			"content": {
			    "application/json": {
				"schema": {
				    "properties": {
					"reason": {
					    "type": "string"
					}
				    }
				}
			    }
			}
		    }
		}
	    }				
	},
	"/health": {
            "get": {
		"x-mojo-to": "meta#health",
		"operationId": "hello",
		"summary": "Get a smiley face",
		"responses": {
		    "200": {
			"description": "It's a smiley face",
			"content": {
			    "application/json": {
				"schema": {
				    "properties": {
					"face": {
					    "type": "string"
					}
				    }
				}
			    }
			}
		    }
		}
	    }
	},
	"/get_token": {
	    "post": {
		"x-mojo-to": "person#login",
		"operationId": "getToken",
		"summary": "Try to get a session token using username/password pair",
		"responses": {
		    "200": {
			"description": "Successful login",
			"content": {
			    "application/json": {
				"schema": {
				    "properties": {
					"token": {
					    "type": "string"
					}
				    }
				}
			    }
			}
		    },
		    "418": {
			"description": "Unsuccessful login",
			"content": {
			    "application/json": {
				"schema": {
				    "properties": {
					"reason": {
					    "type": "string"
					}
				    }
				}
			    }
			}
		    }
		}
	    }
	},
	"/check_token": {
	    "post": {
		"x-mojo-to": "person#check",
		"operationId": "checkToken",
		"summary": "Verify that a token is presently valid.",
		"responses": {
		    "200": {
			"description": "Non-zero value indicates failure reason",
			"content": {
			    "application/json": {
				"schema": {
				    "properties": {
					"code": {
					    "type": "integer"
					}
				    }
				}
			    }
			}
		    },
		    "418": {
			"description": "Unsuccessful login",
			"content": {
			    "application/json": {
				"schema": {
				    "properties": {
					"reason": {
					    "type": "string"
					}
				    }
				}
			    }
			}
		    }	   
		}
	    }
	}
    },
    "servers": [
	{
	    "url": "/api"
	}
    ]
}
			
                  
